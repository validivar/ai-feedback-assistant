cd ai-feedback-assistant

# Create a new vercel.json with unique name
cat > vercel.json << 'EOF'
{
  "version": 2,
  "name": "ai-feedback-$(date +%s)",
  "public": true,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
EOF

# Or simpler version without name
cat > vercel.json << 'EOF'
{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "server.js" }]
}
EOF

# Push changes
git add vercel.json
git commit -m "Update vercel.json with unique name"
git push
